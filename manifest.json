{
    "manifest_version": 2,
    "name": "Auto Redirect to Native Domain",
    "version": "1.0",
    "description": "Automatically redirects www domains to their native domain and enforces HTTPS",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  